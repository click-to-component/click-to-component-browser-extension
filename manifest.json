{
  "default_locale": "en",
  "name": "Click To Component",
  "description": "__MSG_description__",
  "version": "0.0.0.2",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "128": "images/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_scripts/inject_script.js"],
      "all_frames": true
    }
  ],
  "permissions": [],
  "web_accessible_resources": [
    {
      "matches": ["<all_urls>"],
      "resources": ["injected_scripts/click_to_component.js"]
    }
  ]
}
